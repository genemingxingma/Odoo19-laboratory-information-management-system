    <record id="lab_queue_rule_chem_reception" model="lab.department.queue.rule">
        <field name="name">Chemistry Reception Queue</field>
        <field name="department">chemistry</field>
        <field name="role_type">reception</field>
        <field name="priority">all</field>
        <field name="source_model">lab.sample</field>
        <field name="source_state">draft,received</field>
        <field name="include_overdue_only" eval="False"/>
        <field name="action_name">Chemistry Incoming Samples</field>
    </record>
