    <record id="lab_hl7_template_order_default" model="lab.hl7.template">
        <field name="name">Default ORM Order Template</field>
        <field name="code">HL7_ORM_DEFAULT</field>
        <field name="message_type">order</field>
        <field name="version">2.5</field>
    </record>
    <record id="lab_hl7_template_order_seg_msh" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_order_default"/>
        <field name="sequence">10</field>
        <field name="segment_name">MSH</field>
        <field name="field_template">^~\&amp;|LIS|IMYTEST|HIS|HOSPITAL|{{message_time}}||ORM^O01|{{control_id}}|P|2.5</field>
    </record>
    <record id="lab_hl7_template_order_seg_pid" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_order_default"/>
        <field name="sequence">20</field>
        <field name="segment_name">PID</field>
        <field name="field_template">1||{{accession}}||{{patient_name}}</field>
    </record>
    <record id="lab_hl7_template_order_seg_orc" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_order_default"/>
        <field name="sequence">30</field>
        <field name="segment_name">ORC</field>
        <field name="field_template">NW|{{request_no}}</field>
    </record>
    <record id="lab_hl7_template_order_seg_obr" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_order_default"/>
        <field name="sequence">40</field>
        <field name="segment_name">OBR</field>
        <field name="field_template">1|{{request_no}}|{{accession}}|{{service_code}}</field>
    </record>

    <record id="lab_hl7_template_result_default" model="lab.hl7.template">
        <field name="name">Default ORU Result Template</field>
        <field name="code">HL7_ORU_DEFAULT</field>
        <field name="message_type">result</field>
        <field name="version">2.5</field>
    </record>
    <record id="lab_hl7_template_result_seg_msh" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_result_default"/>
        <field name="sequence">10</field>
        <field name="segment_name">MSH</field>
        <field name="field_template">^~\&amp;|LIS|IMYTEST|HIS|HOSPITAL|{{message_time}}||ORU^R01|{{control_id}}|P|2.5</field>
    </record>
    <record id="lab_hl7_template_result_seg_pid" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_result_default"/>
        <field name="sequence">20</field>
        <field name="segment_name">PID</field>
        <field name="field_template">1||{{accession}}||{{patient_name}}</field>
    </record>
    <record id="lab_hl7_template_result_seg_obr" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_result_default"/>
        <field name="sequence">30</field>
        <field name="segment_name">OBR</field>
        <field name="field_template">1|{{request_no}}|{{accession}}|{{service_code}}</field>
    </record>
    <record id="lab_hl7_template_result_seg_obx" model="lab.hl7.template.segment">
        <field name="template_id" ref="lab_hl7_template_result_default"/>
        <field name="sequence">40</field>
        <field name="segment_name">OBX</field>
        <field name="field_template">1|NM|{{service_code}}||{{result_value}}|||{{result_flag}}</field>
        <field name="repeatable" eval="True"/>
    </record>

    <record id="lab_qc_trend_profile_default" model="lab.qc.trend.profile">
        <field name="name">Default QC Trend Profile</field>
        <field name="code">QC_TREND_DEFAULT</field>
        <field name="window_size">20</field>
        <field name="warning_sigma">2.0</field>
        <field name="reject_sigma">3.0</field>
    </record>
</odoo>
